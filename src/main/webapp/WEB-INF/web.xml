<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

	
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UploadImage</servlet-name>
  	<display-name>UploadImage</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.controller.UploadImage</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetImageServlet</servlet-name>
  	<display-name>GetImageServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.controller.GetImageServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteImage</servlet-name>
  	<display-name>DeleteImage</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.controller.DeleteImage</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShowImage</servlet-name>
  	<display-name>ShowImage</display-name>
  	
  	<description></description>
  	<servlet-class>com.nagarro.controller.ShowImage</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateImage</servlet-name>
  	<display-name>UpdateImage</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.controller.UpdateImage</servlet-class>
  </servlet>
 
  
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/loginservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UploadImage</servlet-name>
  	<url-pattern>/uploadImage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetImageServlet</servlet-name>
  	<url-pattern>/getimage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteImage</servlet-name>
  	<url-pattern>/deleteimage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShowImage</servlet-name>
  	<url-pattern>/showimage</url-pattern>
  
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateImage</servlet-name>
  	<url-pattern>/updateimage</url-pattern>
  </servlet-mapping>
 
   <welcome-file-list>
	<welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>